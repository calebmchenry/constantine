{
  "name": "constantine",
  "version": "0.1.0",
  "description": "Transfer constants from one file and copy them to another file",
  "repository": "https://github.com/calebmchenry/constantine",
  "main": "dist/lib/index.js",
  "bin": "dist/bin/index.js",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "bin": "node dist/bin/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "deploy": "npm test && npm run clean && npm run build && npm publish"
  },
  "keywords": [
    "constants",
    "copy",
    "languages"
  ],
  "author": {
    "name": "Caleb McHenry",
    "email": "calebmchenry@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "commander": "^3.0.2"
  }
}
